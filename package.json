{
  "name": "resin-bundle-resolve",
  "version": "3.0.2",
  "description": "Resolve resin-bundles into a docker daemon format",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run clean && tsc --project tsconfig.release.json",
    "build:test": "npm run clean && tsc --project . && cp -r test/test-files build/test",
    "lint": "resin-lint --typescript src/ test/ typings/ && tsc --noEmit",
    "prettify": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write \"{src,test,typings}/**/*.ts\"",
    "test": "npm run build:test && mocha",
    "precommit": "lint-staged",
    "prepublish": "require-npm4-to-publish",
    "prepublishOnly": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io-modules/resin-bundle-resolve.git"
  },
  "author": "Cameron Diver <cameron@resin.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/resin-io-modules/resin-bundle-resolve/issues"
  },
  "homepage": "https://github.com/resin-io-modules/resin-bundle-resolve#readme",
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/chai": "^3.4.35",
    "@types/lodash": "^4.14.52",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.7",
    "@types/request": "0.0.45",
    "@types/semver": "^5.3.32",
    "chai": "^3.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^8.1.0",
    "mocha": "^6.0.0",
    "prettier": "^1.15.3",
    "require-npm4-to-publish": "^1.0.0",
    "resin-lint": "^2.0.1",
    "tar-utils": "^1.1.0",
    "tslint": "^5.5.0",
    "typescript": "^2.1.6"
  },
  "dependencies": {
    "@types/tar-stream": "^1.6.0",
    "bluebird": "^3.4.7",
    "bluebird-lru-cache": "^1.0.0",
    "dockerfile-template": "0.1.0",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "strict-event-emitter-types": "^2.0.0",
    "tar-stream": "^1.6.2"
  }
}
